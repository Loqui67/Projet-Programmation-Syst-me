<Window x:Class="AppWPF.developpement.View.SoftwarePackageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppWPF.developpement.View"
        mc:Ignorable="d"
        Title="softwarePackage" Height="450" Width="800">
    <Grid>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,126,0,10"/>
        <TextBox HorizontalAlignment="Left" Margin="34,80,0,0" TextWrapping="Wrap" Text="Name of process" VerticalAlignment="Top" Width="145" Height="18"/>
        <Label Content="Add the name of the process you consider to be business software:" HorizontalAlignment="Left" Margin="34,32,0,0" VerticalAlignment="Top" Height="27" Width="366"/>
        <Button Content="Submit" HorizontalAlignment="Left" Margin="297,78,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
