<UserControl x:Class="AppWPF.developpement.Components.ProcessusListing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AppWPF.developpement.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <ListView x:Name="listViewProcess" ItemsSource="{Binding ProcessusListingItemViewModels}" Margin="10" Grid.Column="1">
        <ListView.ItemTemplate>
            <DataTemplate>
                <local:ProcessusListingItem/>
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</UserControl>
